set expandtab   " TAB
set smarttab
set tabstop=4
set softtabstop=4
set shiftwidth=4
        
set number  " nomeracuya

set noerrorbells
set novisualbell

syntax on

set mouse=a

set t_Co=256   " This is may or may not needed.
set background=dark
colorscheme PaperColor  " ~/.vim/colors

set ignorecase
set smartcase

set laststatus=2
